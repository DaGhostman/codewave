{
  "name": "codewave/codewave",
  "description": "Small extensible framework in PHP",
  "keywords": ["CodeWave", "PHP Framework", "MVC Framework"],
  "homepage": "https://github.com/DaGhostman/codewave",
  "type": "library",
  "require": {
    "php": ">=5.4",
    "psr/http-message": "1.0",
    "nikic/fast-route": "v0.6.0",
    "goaop/framework": "0.6.1",
    "doctrine/annotations": "1.*@stable",
    "zendframework/zend-diactoros": "1.*"
  },
  "prefer-stable": true,
  "suggest": {
    "monolog/monolog": "PSR-3 Logger implementation",
    "php-di/php-di": "Dependency Injection Container",
    "doctrine/cache": "A High performance caching library",
    "doctrine/orm": "High Performing ORM"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Dimitar Dimitrov",
      "email": "daghostman.dimitrov@gmail.com",
      "role": "development"
    }
  ],
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": { "Wave\\Framework\\": "src" }
  },
  "autoload-dev": {
    "psr-0": {
      "Stub\\": "tests",
      "Test\\": "tests"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "4.*",
    "mockery/mockery": "~0.9",
    "squizlabs/php_codesniffer": "2.*",
    "phpmd/phpmd": "2.*",
    "satooshi/php-coveralls": "dev-master"
  }
}
