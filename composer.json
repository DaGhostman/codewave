{
    "name": "codewave/codewave",
    "description": "Small extensible framework in PHP",
    "keywords": ["CodeWave", "PHP Framework", "MVC Framework"],
    "homepage": "https://github.com/DaGhostman/codewave",
    "type": "library",
    "require": {
        "php": ">=5.4",
        "zendframework/zend-log": ">=2.0",
        "phroute/phroute": ">=1.0",
        "psr/http-message": "*@stable"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Dimitar Dimitrov",
            "email": "daghostman.dimitrov@gmail.com",
            "role": "development"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-0": { "Wave\\Framework\\": "src", "Stub\\": "tests" }
    },
    "suggest": {
        "pimple/pimple": "For dependency injection container"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "mockery/mockery": ">=0.9",
        "squizlabs/php_codesniffer": ">1.5",
        "satooshi/php-coveralls": "dev-master"
    }
}
