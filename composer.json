{
  "name": "codewave/codewave",
  "description": "Small extensible framework in PHP",
  "keywords": ["CodeWave", "PHP Framework", "MVC Framework"],
  "homepage": "https://github.com/DaGhostman/codewave",
  "type": "library",
  "require": {
    "php": ">=5.4",
    "nikic/fast-route": "v0.6.0"
  },
  "prefer-stable": true,
  "suggest": {
    "psr/log": "1.0.0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Dimitar Dimitrov",
      "email": "daghostman.dimitrov@gmail.com",
      "role": "development"
    }
  ],
  "minimum-stability": "stable",
  "autoload": {
    "psr-0": { "Wave\\Framework\\": "src" }
  },
  "autoload-dev": {
    "psr-0": {
      "Stub\\": "tests",
      "Test\\": "tests"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "~4.7",
    "mockery/mockery": "~0.9",
    "squizlabs/php_codesniffer": "~2.3",
    "phpmd/phpmd": "~2.2",
    "satooshi/php-coveralls": "dev-master"
  }
}
